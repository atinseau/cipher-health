{
  "name": "cipher-health",
  "private": true,
  "scripts": {
    "up": "docker-compose -f docker/docker-compose.yml up --build -d",
    "down": "docker-compose -f docker/docker-compose.yml down",
    "predev": "pnpm run up",
    "dev": "FORCE_COLOR=true turbo dev --no-daemon",
    "clean": "FORCE_COLOR=true turbo clean --no-daemon",
    "fclean": "FORCE_COLOR=true turbo fclean --no-daemon && rm -rf node_modules .turbo pnpm-lock.yaml",
    "build": "FORCE_COLOR=true turbo build --no-daemon",
    "start": "FORCE_COLOR=true turbo start --no-daemon",
    "test": "FORCE_COLOR=true turbo test --no-daemon",
    "check-types": "FORCE_COLOR=true turbo check-types --no-daemon",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "1.11.3",
    "turbo": "1.11.3",
    "husky": "8.0.3"
  }
}

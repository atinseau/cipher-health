{
  "name": "cipher-health",
  "private": true,
  "scripts": {
    "up": "docker-compose -f docker/docker-compose.yml up --build -d",
    "down": "docker-compose -f docker/docker-compose.yml down",
    "predev": "pnpm run up",
    "dev": "FORCE_COLOR=true turbo dev --filter=!@cipher-health/frontend",
    "clean": "FORCE_COLOR=true turbo clean --filter=!@cipher-health/frontend",
    "fclean": "FORCE_COLOR=true turbo fclean --filter=!@cipher-health/frontend && rm -rf node_modules .turbo pnpm-lock.yaml",
    "build": "FORCE_COLOR=true turbo build --filter=!@cipher-health/frontend",
    "test": "FORCE_COLOR=true turbo test --filter=!@cipher-health/frontend",
    "check-types": "FORCE_COLOR=true turbo check-types --filter=!@cipher-health/frontend",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "1.11.2",
    "turbo": "1.10.16",
    "husky": "8.0.3"
  }
}

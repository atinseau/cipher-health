{
  "name": "@cipher-health/utils",
  "private": true,
  "description": "Esm and Commonjs utils for Cipher Health",
  "version": "0.0.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "dev:swc:commonjs": "swc --watch --out-dir dist/commonjs src -C module.type=commonjs",
    "dev:swc:esm": "swc --watch --out-dir dist/esm src -C module.type=es6",
    "build:swc:commonjs": "swc --out-dir dist/commonjs src -C module.type=commonjs",
    "build:swc:esm": "swc --out-dir dist/esm src -C module.type=es6",
    "dev:ts": "tsc --watch --emitDeclarationOnly -p tsconfig.build.json | sed -e 's/\\x1Bc//g'",
    "build:ts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "dev": "run-p dev:swc:commonjs dev:swc:esm dev:ts",
    "build": "run-p build:swc:esm build:swc:commonjs build:ts",
    "clean": "rm -rf dist",
    "fclean": "rm -rf dist node_modules .turbo"
  },
  "devDependencies": {
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@types/jest": "29.5.11",
    "typescript": "5.3.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": "test",
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
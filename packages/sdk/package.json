{
  "name": "@cipher-health/sdk",
  "version": "0.0.0",
  "description": "Esm SDK for Cipher Health",
  "private": true,
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./react": "./src/react/index.ts"
  },
  "typesVersions": {
    "*": {
      "react": [
        "./src/react/index.ts"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "check-types": "tsc --noEmit",
    "fclean": "rm -rf node_modules .turbo"
  },
  "dependencies": {
    "async-mutex": "0.4.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@cipher-health/api": "workspace:*",
    "typescript": "5.3.3",
    "@types/jest": "29.5.11",
    "@types/react": "18.2.46",
    "jest": "29.7.0",
    "ts-jest": "29.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": "test",
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
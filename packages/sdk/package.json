{
  "name": "@cipher-health/sdk",
  "version": "0.0.0",
  "description": "Esm SDK for Cipher Health",
  "private": true,
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react/index.js"
  },
  "typesVersions": {
    "*": {
      "react": [
        "./dist/react/index.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "fclean": "rm -rf dist node_modules .turbo",
    "dev:swc": "swc --watch --out-dir dist src",
    "build:swc": "swc --out-dir dist src",
    "dev:ts": "tsc --watch --emitDeclarationOnly -p tsconfig.build.json | sed -e 's/\\x1Bc//g'",
    "build:ts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "dev": "run-p dev:swc dev:ts",
    "build": "run-p build:swc build:ts"
  },
  "dependencies": {
    "async-mutex": "0.4.0",
    "react": "18.2.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@cipher-health/api": "workspace:*",
    "@cipher-health/utils": "workspace:*",
    "@types/jest": "29.5.11",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@types/react": "18.2.46",
    "npm-run-all": "4.1.5",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": "test",
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
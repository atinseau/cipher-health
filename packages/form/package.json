{
  "name": "@cipher-health/form",
  "private": true,
  "description": "A form library build on top of react-hook-form",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "fclean": "rm -rf dist node_modules .turbo",
    "dev:swc": "swc --watch --out-dir dist src",
    "build:swc": "swc --out-dir dist src",
    "dev:ts": "tsc --watch --emitDeclarationOnly | sed -e 's/\\x1Bc//g'",
    "build:ts": "tsc --emitDeclarationOnly",
    "dev": "run-p dev:swc dev:ts",
    "build": "run-p build:swc build:ts"
  },
  "dependencies": {
    "@cipher-health/utils": "workspace:*",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-hook-form": "7.49.2"
  },
  "devDependencies": {
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.101",
    "@types/lodash": "4.14.202",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "npm-run-all": "4.1.5",
    "typescript": "5.3.3"
  }
}
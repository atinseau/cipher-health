{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "fclean": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "check-types": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}